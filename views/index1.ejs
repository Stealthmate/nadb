<html lang="en-US">

	<head>

		<meta charset="utf-8">
		
		</meta>
	
		<title>Shippuuden-Arena</title>
		
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
		
		<script type="text/javascript" src="resource/scripts/queryUpdate.js"> </script>
		<script type="text/javascript" src="resource/scripts/characterinfo.js"> </script>
		<script type="text/javascript" src="resource/scripts/ability.js"> </script>
		
		<link rel="stylesheet" type="text/css" href="resource/styles/style_index.css">
		<link rel="stylesheet" type="text/css" href="resource/styles/infopane1.css">
		<link rel="stylesheet" type="text/css" href="resource/styles/descriptionpane.css">
		
	</head>

	
	<body onkeydown="onKeyDown();">
		<div id="searchpane">
			<form style="margin-bottom: 1px;">
				<input 
				id="searchbox" 
				name="characterQuery" 
				type="text" 
				autocomplete="off"
				placeholder="Character name"
				oninput    = "updateSuggestions();"></input>
			</form>
			
			<ol id="resultlist">
			</ol>
		</div>

	<div id="infopane">
	
		<div id="teamspane">
		
			<div class="team" id="player">
				<%- include("team"); %>
				
			</div>
			
			<div class="team" id="enemy" dir="rtl">
				<%- include("team"); %>
			</div>
		
		</div>
		
		<div id="descriptionpane"> 
		
			<div class="header">
			
				<img class="icon"></img>
				
				<div class="basicinfo">
					
					<div class="name">Ability</div>
					<div class="description"></div>
					
					
				</div>
				
			
			</div>
			
			<div class="stats">
			
				<span class="label">
					Cost:
					<table class="cost" nocost="true">
						<td class="abilitycost tooltipped" id="n"><span class="tooltip"></span></td>
					</table>
					<span class="nocost">
						None
					</span>
				</span>
				
				
				<span class="label">
					Cooldown: <span class="cd"></span>
				</span>
				
				<table class="classes">			
				</table>
			</div>
			
		</div>
	
	</div>
	
	</body>
</html>